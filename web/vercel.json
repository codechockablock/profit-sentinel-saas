{
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.profitsentinel.com"
  },
  "rewrites": [
    {
      "source": "/api/diagnostic/:path*",
      "destination": "https://api.profitsentinel.com/diagnostic/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?profitsentinel\\.com"
        }
      ]
    },
    {
      "source": "/api/premium/:path*",
      "destination": "https://api.profitsentinel.com/premium/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?profitsentinel\\.com"
        }
      ]
    },
    {
      "source": "/api/:path*",
      "destination": "https://api.profitsentinel.com/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?profitsentinel\\.com"
        }
      ]
    }
  ]
}
